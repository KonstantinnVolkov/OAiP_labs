unit API;

interface

Type
  TUsefulInfo = record
    DataType: String[80];
    Name: String[80];
    Author: String[80];
    cost: String[80];
    Other: String[80];
  end;

  ptr = ^Element;

  Element = record
    next, prev: ptr;
    data: TUsefulInfo;
  end;

  List = record
    Head, Tail: ptr;
  end;

implementation

end.
